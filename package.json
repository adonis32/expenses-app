{
  "name": "expenses",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@chakra-ui/icons": "^1",
    "@chakra-ui/react": "^1.0.0",
    "@chakra-ui/theme-tools": "^2.0.2",
    "@emotion/react": "^11.0.0",
    "@emotion/styled": "^11.0.0",
    "dinero.js": "^1.9.1",
    "firebase": "^9.8.4",
    "focus-visible": "^5.2.0",
    "framer-motion": "^4.0.0",
    "nanoid": "^4.0.0",
    "react": "^17.0.0",
    "react-component-pack": "^1.0.1",
    "react-currency-input-field": "^3.6.4",
    "react-dom": "^17.0.0",
    "react-feather": "^2.0.3",
    "react-hyper-scroller": "^3.0.1",
    "react-router-dom": "^5.1.2",
    "typescript": "~4.7.4"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint:base": "eslint --ext .ts,.tsx",
    "lint": "pnpm lint:base src",
    "fmt": "pnpm lint:base --fix && prettier --write 'src/**/*.{ts,tsx,css}'",
    "prepare": "husky install",
    "test": "vitest"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "pnpm lint:base --fix",
      "prettier --write"
    ]
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "import/no-anonymous-default-export": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/dinero.js": "^1.9.0",
    "@types/nanoid": "^3.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.3",
    "@vitejs/plugin-react": "^1.3.2",
    "eslint": "^8.0.0",
    "eslint-config-react-app": "^7.0.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "vite": "^2.9.13",
    "vite-plugin-pwa": "^0.12.2",
    "vitest": "^0.18.0"
  },
  "pnpm": {
    "patchedDependencies": {
      "@chakra-ui/image@1.1.10": "patches/@chakra-ui__image@1.1.10.patch"
    }
  }
}
